class Person {
    name: String;
    mother: Person | Nil;
    father: Person | Nil;
    init(name: String, mother: Person | Nil, father: Person | Nil) {
        this.name = name;
        this.mother = mother;
        this.father = father;
    }
}
var mum = Person("Beth", nil, nil);
var daughter = Person("Jane", mum, nil);
