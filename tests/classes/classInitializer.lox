class City {
    name: String;
    country: String;

    init(name: String, country: String) {
        this.name = name;
        this.country = country;
        print("initialized!");
    }
    location(): String {
        return this.name + ", " + this.country;
    }
}
var ny = City("New York", "USA");
print ny.location();
-- OUTPUT --
initialized!
New York, USA
